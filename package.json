{
  "name": "nextjs-contentful-storybook-setup",
  "version": "0.0.1",
  "author": "Tim Huijg <tim_huijg@hotmail.com>",
  "private": true,
  "scripts": {
    "watch": "run-p watch:*",
    "build": "run-s build:next build:storybook",
    "release": "run-s release:next",
    "watch:next": "next dev -p 3200",
    "build:next": "next build",
    "release:next": "npm run build:next",
    "watch:storybook": "start-storybook -p 3100 -s public",
    "build:storybook": "build-storybook -s public",
    "generate-icons": "npx @svgr/cli --template ./icon-template.js --index-template ./icon-index-template.js --filename-case kebab --svg-props 'className={className}' --replace-attr-values '#000={fill}' --icon -d ./src/components/icons ./assets/icons"
  },
  "dependencies": {
    "@babel/helper-regex": "^7.10.5",
    "classnames": "^2.2.6",
    "contentful": "^7.14.6",
    "invariant": "^2.2.4",
    "next": "10.0.3",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.10",
    "sass-resources-loader": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@svgr/cli": "^5.4.0",
    "@touchtribe/babel-presets": "^0.1.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.1.0",
    "storybook-addon-next-router": "^2.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0"
  }
}
